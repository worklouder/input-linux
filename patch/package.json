{
  "name": "input",
  "version": "0.8.0-rc.4-DevBuild",
  "description": "Input App - Work Louder",
  "author": "Work Louder",
  "license": "none",
  "private": true,
  "type": "module",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "build": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@electron/notarize": "^2.3.2",
    "@tippyjs/react": "^4.2.6",
    "crypto-js": "^4.2.0",
    "css-modules": "^0.3.0",
    "electron-dl": "^4.0.0",
    "electron-log": "^5.2.0",
    "electron-notarize": "^1.2.2",
    "electron-updater": "^6.1.8",
    "esptool-js": "^0.4.3",
    "node-hid": "^3.1.2",
    "octokit": "^4.0.2",
    "react-draggable": "^4.4.6",
    "react-easy-crop": "^5.2.0",
    "react-markdown": "^9.0.3",
    "react-modal": "^3.16.3",
    "react-router-dom": "^6.22.3",
    "recoil": "^0.7.7",
    "recoil-persist": "^5.1.0",
    "serialport": "^12.0.0",
    "sudo-prompt": "^9.2.1",
    "web-serial-polyfill": "^1.0.15"
  },
  "devDependencies": {
    "electron": "^29.2.0",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "worklouder",
        "repo": "input-linux"
      }
    ],
    "appId": "com.worklouder.input",
    "productName": "Input",
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "files": [
      "dist/**",
      "dist-electron/**"
    ],
    "extraFiles": [
      {
        "from": "AppRun",
        "to": "AppRun",
        "filter": ["AppRun"]
      }
    ],
    "asarUnpack": [
      "dist-electron/scripts/install-udev-worklouder.sh"
    ],
    "linux": {
      "target": "AppImage",
      "category": "Utility",
      "icon": "dist/assets/appicon.png",
      "executableName": "input"
    },
    "npmRebuild": false,
    "buildDependenciesFromSource": false,
    "extraMetadata": {
      "main": "dist-electron/main/index.js"
    }
  }
}